// Fibonacci numbers

num = ?;

if (num == 0)
{
    print 0;
}

if ((num == 1) || (num == 2))
{
    print 1;
}

if (num > 2)
{
    first  = 1;
    second = 1;
    third  = 2;

    while (num > 2)
    {
        third = first + second;
        first = second;
        second = third;
        num = num - 1;
    }

    print third;
}

